{
  "id": "Test-iz3awm",
  "name": "Белая ворона",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "24.7.0"
    }
  ],
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "localFileSystem": "fullAccess",
    "launchProcess": {
      "schemes": [
        "http",
        "https"
      ],
      "extensions": [
        ".svg",
        ".png"
      ]
    },
    "webview": {
      "allow": "yes",
      "domains": [
        "https://*.adobe.com",
        "https://*.google.com"
      ]
    }
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "showAbout",
      "label": {
        "default": "About React Demo"
      },
      "shortcut": {
        "mac": "shift+cmd+A",
        "win": "ctrl+alt+A"
      }
    },
    {
      "type": "panel",
      "id": "demos",
      "label": {
        "default": "Белая ворона"
      },
      "minimumSize": {
        "width": 600,
        "height": 650
      },
      "maximumSize": {
        "width": 600,
        "height": 650
      },
      "preferredDockedSize": {
        "width": 600,
        "height": 650
      },
      "preferredFloatingSize": {
        "width": 600,
        "height": 650
      },
      "icons": [{
        "width": 23,
        "height": 23,
        "path": "icons/light.png",
        "scale": [1, 2],
        "theme": ["darkest", "dark"]
      }, {
        "width": 23,
        "height": 23,
        "path": "icons/dark.png",
        "scale": [1, 2],
        "theme": ["lightest", "light"]
      }]
    }
  ],
  "icons": [{
    "width": 48,
    "height": 48,
    "path": "icons/lightPlugin.png",
    "scale": [1, 2],
    "theme": ["darkest", "dark"],
    "species": ["pluginList"]
  }, {
    "width": 48,
    "height": 48,
    "path": "icons/darkPlugin.png",
    "scale": [1, 2],
    "theme": ["lightest", "light"],
    "species": ["pluginList"]
  }]
}